{
  "name": "bolt-figma",
  "version": "0.0.10",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperbrew/bolt-figma.git"
  },
  "homepage": "https://github.com/hyperbrew/bolt-figma",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch",
    "devcode": "vite build --watch --config vite.config.code.ts ",
    "build": "vite build",
    "buildcode": "vite build --config vite.config.code.ts",
    "preview": "vite preview",
    "hmr": "vite",
    "zip": "cross-env MODE=zip vite build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dep": "cross-env ACTION=dependencyCheck vite",
    "example": "cross-env ACTION=example vite"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.94.0",
    "@sveltejs/vite-plugin-svelte": "^2.4.2",
    "@tsconfig/svelte": "^5.0.0",
    "@types/node": "^16.7.1",
    "sass": "^1.75.0",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.6",
    "typescript": "^5.0.2",
    "vite": "^5.2.12",
    "vite-figma-plugin": "^0.0.7",
    "vite-plugin-singlefile": "^0.13.5"
  },
  "dependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "cross-env": "^7.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vue": "^3.4.27"
  },
  "files": [
    "index.html",
    "package.json",
    "package.react.jsonc",
    "package.svelte.jsonc",
    "package.vue.jsonc",
    "public/**/*",
    "public-zip/**/*",
    "README.md",
    "src/**/*",
    "src-code/**/*",
    "tsconfig.json",
    "figma.config.ts",
    "vite.config.ts",
    "vite.config.code.ts",
    "yarn.lock",
    ".gitignore"
  ]
}
